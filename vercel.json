{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/slide/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/slide/index.html"
    },
    {
      "source": "/slides",
      "destination": "/slide/index.html"
    },
    {
      "source": "/peatix",
      "destination": "/slide/index.html"
    }
  ]
}